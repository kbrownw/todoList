<%- include("header") -%>
  
    <div class="box" id="heading">
      <h1><%= listTitle %></h1>
    </div>
    
    <div class="box">
        <% for (let i=0; i<itemArray.length; i++) { %>
          <div class="item">
            <input type="checkbox">
            <p><%= itemArray[i] %></p>
          </div>
        <% } %>

        <form class="item" action="/" method="POST">
          <input type="text" id="additem" name="additem" placeholder="New Item" autocomplete="off"/>
          <button type="submit" id="submitbtn" name="list" value=<%= listTitle %>>+</button>
        </form>
    </div>
  
<%- include("footer") -%>